    "prepare": "npx husky install",
    "precommit": "lint-staged",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx src/ --fix",
    "postinstall": "node postinstall.js",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json,scss} --config ./.prettierrc.json"
  },
  "dependencies": {
