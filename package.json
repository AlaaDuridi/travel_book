{
  "name": "travel-and-accomodation-booking-platform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "chmod +x ./env.sh && ./env.sh && cp env-config.js ./public/ && vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest --coverage",
